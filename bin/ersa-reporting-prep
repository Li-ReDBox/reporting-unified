#!/usr/bin/env python3
"""app entry point."""

# pylint: disable=invalid-name

import sys

if len(sys.argv) != 2:
    print("args: package")
    sys.exit(1)

package = sys.argv[1]

module = __import__(package)

module.db.engine.execute("create extension if not exists \"uuid-ossp\";")
module.db.create_all()
